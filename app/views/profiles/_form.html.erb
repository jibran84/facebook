
<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <!-- <%= f.association :user %> deleted because we don't want the user to see this-->
    <%= f.input :name %>
    <%= f.input :picture %>
    <%= f.input :bio %>
    <%= f.input :country %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
